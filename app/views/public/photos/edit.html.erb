<div class="container">
  <div class="row">
    <div class="col">
      投稿編集
    </div>
  </div>
  <%= form_with model: @photo, url: photo_path do |f| %>
  <div class="row">
    <div class="col">
      <%= f.label "写真" %>
    </div>
    <div class="col">
      <%= f.file_field :image, accept: "image/*" %>
    </div>
  </div>
  <div class="row">
    <div class="col">
      <%= f.label "住所" %>
    </div>
    <div class="col">
      <%= f.text_field :address %>
    </div>
  </div>
  <div class="row">
    <div class="col">
      <%= f.label "コメント" %>
    </div>
    <div class="col">
      <%= f.text_field :comment %>
    </div>
  </div>
  <div class="row">
    <div class="col">
      <%= f.submit "保存する" %>
    </div>
  </div>
  <% end %>
</div>