<div class="container">
  <div class="row">
    <div class="">
      ユーザー一覧
    </div>
  </div>
  <div class="row">
    <div class="">
      <table class="table">
        <thead>
          <tr>
            <th>ユーザー名</th>
            <th>メールアドレス</th>
            <th>ステータス</th>
          </tr>
        </thead>
        <% @users.each do |user| %>
        <tbody>
          <tr>
            <td><%= user.name %></td>
            <td><%= user.email %></td>
            <td><%= user.is_deleted %></td>
          </tr>
        </tbody>
        <% end %>
      </table>
    </div>
  </div>
</div>