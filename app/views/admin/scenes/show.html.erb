<div class="container">
  <div class="row">
    <div class="col">
      シーン詳細
    </div>
  </div>
  <div class="row">
    <div class="col">
      シーン名
    </div>
    <div class="col">
      <%= @scene.scene_name %>
    </div>
  </div>
  <div class="row">
    <div class="col">
      住所
    </div>
    <div class="col">
      <%= @scene.scene_address %>
    </div>
  </div>
  <div class="row">
    <div class="col">
      <%= link_to "編集する", edit_admin_scene_path %>
    </div>
  </div>
  <div class="row">
    <div class="col">
      投稿一覧
    </div>
  </div>
  <div class="row">
    <% @photos.each do |photo| %>
    <div class="col">
      <%= link_to admin_photo_path(photo.id) do %>
      <%= image_tag photo.get_image(100,100) %>
      <% end %>
    </div>
    <% end %>
  </div>
</div>