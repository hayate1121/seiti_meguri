<div class="container">
  <div class="row">
    <div class="col">
      シーン情報編集
    </div>
  </div>
  <%= form_with model: @scene, url: admin_scene_path do |f| %>
  <div class="row">
    <div class="col">
      <%= f.label "シーン名" %>
    </div>
    <div class="col">
      <%= f.text_field :scene_name %>
    </div>
  </div>
  <div class="row">
    <div class="col">
      <%= f.label "住所" %>
    </div>
    <div class="col">
      <%= f.text_field :scene_address %>
    </div>
  </div>
  
  
  <div class="row">
    <div class="col">
      <%= f.submit "保存する" %>
    </div>
  </div>
  <% end %>
</div>