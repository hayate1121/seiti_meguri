<div class="container">
  <div class="row">
    <div class="">
      シーン追加
    </div>
  </div>
  <%= form_with model: @scene do |f| %>
  <div class="row">
    <div class="col">
      <%= f.label "シーン名" %>
    </div>
    <div class="col">
      <%= f.text_field :scene_name %>
    </div>
  </div>
  
  <div class="row">
    <div class="col">
      <%= f.submit "追加する" %>
    </div>
  </div>
  <% end %>
</div>