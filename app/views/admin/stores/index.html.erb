<div class="container">
  <div class="row">
    <div class="col">
      店舗一覧
    </div>
  </div>
  <div class="row">
    <div class="col">
      <table class="table">
        <thead>
          <tr>
            <th>住所</th>
            <th>店舗名</th>
          </tr>
        </thead>
        <% @stores.each do |store| %>
        <tbody>
          <tr>
            <td><%= store.store_address %></td>
            <td><%= store.store_name %></td>
          </tr>
        </tbody>
        <% end %>
      </table>
    </div>
  </div>
</div>