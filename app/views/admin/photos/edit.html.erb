<div class="container">
  <div class="row">
    <div class="">
      投稿編集
    </div>
  </div>
  <%= form_with model: @photo do |f| %>
  
  <div class="row">
    <div class="">
      <%= f.label "住所" %>
    </div>
    <div class="">
      <%= f.text_field %>
    </div>
  </div>
  <div class="row">
    <div class="">
      <%= f.label "コメント" %>
    </div>
    <div class="">
      <%= f.text_area :comment %>
    </div>
  </div>
  <div class="row">
    <div class="">
      <%= f.submit "保存する" %>
    </div>
  </div>
  <% end %>
</div>